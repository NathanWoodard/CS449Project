<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fitnesslog">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".WorkoutLog"
            android:label="@string/title_activity_workout_log"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".SleepLog"
            android:label="@string/title_activity_sleep_log"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CalorieLog"
            android:label="@string/title_activity_calorie_log"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".WeightLog"
            android:label="WeightLog"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Weight" />
        <activity android:name=".Sleep" />
        <activity android:name=".Workout" />
        <activity android:name=".Calorie" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>